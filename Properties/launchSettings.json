{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "AzureCustomResourceProviderRESTAPI": {
      "commandName": "Project",
      "launchUrl": "customresource",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
      },
      "applicationUrl": "https://localhost/"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/customresource",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:443;http://+:80",
        "ASPNETCORE_HTTPS_PORT": "44365"
      },
      "httpPort": 10000,
      "useSSL": true,
      "sslPort": 44365
    }
  }
}